<interface>
  <requires lib="gtk" version="4.8"/>
  <requires lib="libadwaita" version="1.4"/>
  <template class="DiskSelector" parent="AdwNavigationPage">
    <property name="title">Select disk</property>
    <child>
      <object class="AdwStatusPage">
        <property name="child">
          <object class="GtkBox">
            <property name="orientation">vertical</property>
            <child>
              <object class="GtkListBox" id="DiskList"/>
            </child>
            <child>
              <object class="GtkCheckButton" id="OEMInstall">
                <property name="label">OEM installation</property>
              </object>
            </child>
          </object>
        </property>
        <property name="description">Choose the destination disk</property>
        <property name="icon-name">drive-harddisk-symbolic</property>
      </object>
    </child>
  </template>
</interface>
