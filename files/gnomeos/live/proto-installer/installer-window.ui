<?xml version='1.0' encoding='UTF-8'?>
<!-- Created with Cambalache 0.94.1 -->
<interface>
  <requires lib="gtk" version="4.6"/>
  <requires lib="libadwaita" version="1.4"/>
  <template class="InstallerWindow" parent="AdwApplicationWindow">
    <property name="default-height">600</property>
    <property name="default-width">800</property>
    <child>
      <object class="AdwToolbarView" id="ToolbarView">
        <child type="top">
          <object class="AdwHeaderBar" id="Header">
            <property name="title-widget">
              <object class="AdwWindowTitle">
                <property name="title" translatable="yes">Install GNOME OS</property>
              </object>
            </property>
          </object>
        </child>
        <child>
          <object class="AdwNavigationView" id="NavigationView"/>
        </child>
      </object>
    </child>
  </template>
</interface>
