[Desktop Entry]
Name=GNOME OS proto-installer
Comment=Installs GNOME OS onto a disk
Exec=/usr/lib/gnomeos-installer/installer.py
Terminal=false
Type=Application
StartupNotify=true
OnlyShowIn=GNOME;
Categories=System;GNOME;GTK;
X-GNOME-UsesNotifications=true
Actions=oem-install;

[Desktop Action oem-install]
Exec=/usr/lib/gnomeos-installer/installer.py --oem-mode
Name=OEM installation
