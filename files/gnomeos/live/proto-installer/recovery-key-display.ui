<interface>
  <requires lib="gtk" version="4.6"/>
  <template class="RecoveryKeyDisplay" parent="GtkCenterBox">
    <child type="center">
      <object class="GtkCenterBox">
        <property name="orientation">vertical</property>
        <child type="center">
          <object class="GtkBox">
            <property name="orientation">vertical</property>
            <child>
              <object class="GtkLabel">
                <property name="label">Your disk is encrypted!</property>
              </object>
            </child>
            <child>
              <object class="GtkLabel">
                <property name="label">Keep this recovery on another medium:</property>
              </object>
            </child>
            <child>
              <object class="GtkLabel" id="RecoveryKey">
                <property name="selectable">True</property>
              </object>
            </child>
            <child>
              <object class="GtkLabel" id="InstallationStatus"/>
            </child>
          </object>
        </child>
      </object>
    </child>
  </template>
</interface>
