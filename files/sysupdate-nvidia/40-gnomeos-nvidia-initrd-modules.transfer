[Transfer]
ProtectVersion=%A
Features=nvidia-driver

[Source]
Type=url-file
Path=https://1270333429.rsc.cdn77.org/nightly/sysupdate/
MatchPattern=nvidia-modules-%a_@v.raw \
             nvidia-modules-%a_@v.raw.xz \
             nvidia-modules-%a_@v.raw.zst

[Target]
Type=regular-file
Path=/EFI/Linux
PathRelativeTo=boot
MatchPattern=gnomeos_@v.efi.extra.d/nvidia-modules.raw
Mode=0444
InstancesMax=2
