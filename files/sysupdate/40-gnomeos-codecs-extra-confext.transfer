[Transfer]
ProtectVersion=%A
Features=codecs-extra

[Source]
Type=url-file
Path=https://1270333429.rsc.cdn77.org/nightly/sysupdate/
MatchPattern=codecs-extra-confext-%a_@v.raw \
             codecs-extra-confext-%a_@v.raw.xz \
             codecs-extra-confext-%a_@v.raw.zst

[Target]
Type=regular-file
Path=/var/lib/confexts/
MatchPattern=codecs-extra_@v.raw
InstancesMax=2
