[Transfer]
ProtectVersion=%A
Features=devel

[Source]
Type=url-file
Path=https://1270333429.rsc.cdn77.org/nightly/sysupdate/
MatchPattern=devel-confext-%a_@v.raw \
             devel-confext-%a_@v.raw.xz \
             devel-confext-%a_@v.raw.zst

[Target]
Type=regular-file
Path=/var/lib/confexts/
MatchPattern=devel_@v.raw
InstancesMax=2
