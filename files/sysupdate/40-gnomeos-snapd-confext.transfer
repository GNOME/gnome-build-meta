[Transfer]
ProtectVersion=%A
Features=snapd

[Source]
Type=url-file
Path=https://1270333429.rsc.cdn77.org/nightly/sysupdate/
MatchPattern=snapd-confext-%a_@v.raw \
             snapd-confext-%a_@v.raw.xz \
             snapd-confext-%a_@v.raw.zst

[Target]
Type=regular-file
Path=/var/lib/confexts/
MatchPattern=snapd_@v.raw
InstancesMax=2
