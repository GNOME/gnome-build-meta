[Transfer]
ProtectVersion=%A
Features=snapd

[Source]
Type=url-file
Path=https://1270333429.rsc.cdn77.org/nightly/sysupdate/
MatchPattern=snapd-%a_@v.raw \
             snapd-%a_@v.raw.xz \
             snapd-%a_@v.raw.zst

[Target]
Type=regular-file
Path=/var/lib/extensions/
MatchPattern=snapd_@v.raw
InstancesMax=2
